{
  "name": "htmv",
  "displayName": "htmv",
  "description": "HTMV's templating language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "htmv",
      "aliases": ["HTMV", "htmv"],
      "extensions": [".htmv"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "htmv",
      "scopeName": "text.html.htmv",
      "path": "./syntaxes/htmv.tmLanguage.json"
    }]
  },
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "vscode": "^1.1.37"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
